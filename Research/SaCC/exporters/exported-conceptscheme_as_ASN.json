    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-6bfa67dc-faaf-47ee-890b-f2f72d455b14",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.936Z",
      "ceterms:ctid": "ce-6bfa67dc-faaf-47ee-890b-f2f72d455b14",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-6bfa67dc-faaf-47ee-890b-f2f72d455b14"
      ],
      "skos:definition": {
        "en-us": "Demonstrates achievement of fundamental competencies to complete narrow and limited tasks within a highly structured field of study or work under direct supervision or guidance."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 1"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-305c2136-d08a-4f7d-bb72-d9efc588ffc7",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.942Z",
      "ceterms:ctid": "ce-305c2136-d08a-4f7d-bb72-d9efc588ffc7",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-305c2136-d08a-4f7d-bb72-d9efc588ffc7"
      ],
      "skos:definition": {
        "en-us": "Demonstrates achievement of fundamental competencies to complete technical, routine tasks within a structured field of study or work largely subject to overall direction or guidance."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 2"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-1cc7dca2-03bd-4223-931e-f89ae74a217e",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.945Z",
      "ceterms:ctid": "ce-1cc7dca2-03bd-4223-931e-f89ae74a217e",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-1cc7dca2-03bd-4223-931e-f89ae74a217e"
      ],
      "skos:definition": {
        "en-us": "Demonstrates competencies for processing well-defined technical tasks that are less structured and include non-routine tasks. These tasks have some degree of complexity, assigned within a comprehensive field of study or occupational activity subject to some change and largely subject to overall supervision or guidance."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 3"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-91a1ed6d-5de8-4341-b068-f2d5d1dc5b34",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.951Z",
      "ceterms:ctid": "ce-91a1ed6d-5de8-4341-b068-f2d5d1dc5b34",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-91a1ed6d-5de8-4341-b068-f2d5d1dc5b34"
      ],
      "skos:definition": {
        "en-us": "Demonstrates competencies for the processing of specialized and complex tasks within a comprehensive field of study or an occupational environment that is subject to change. This requires theoretical knowledge and practical skills to select appropriate principles and procedures and may involve overall supervision."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 4"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-e58703f7-15f3-44cd-a039-509896c0ec6e",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.954Z",
      "ceterms:ctid": "ce-e58703f7-15f3-44cd-a039-509896c0ec6e",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-e58703f7-15f3-44cd-a039-509896c0ec6e"
      ],
      "skos:definition": {
        "en-us": "Demonstrates advanced competencies for the processing of comprehensive tasks assigned within a complex and specialized field of study or occupational activity subject to change. This requires the ability to select and apply appropriate theoretical knowledge and practical skills to perform technical tasks in a broad range of contexts"
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 5"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-51f8f716-401e-44ae-bd80-b4335b80a5e4",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.957Z",
      "ceterms:ctid": "ce-51f8f716-401e-44ae-bd80-b4335b80a5e4",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-51f8f716-401e-44ae-bd80-b4335b80a5e4"
      ],
      "skos:definition": {
        "en-us": "Demonstrates mastery in the processing of comprehensive tasks and problems within subareas of a field of study or within a field of occupational activity characterized by a high degree of complexity and by frequent changes. This requires a high degree of theoretical knowledge and practical skills."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 6"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-d7acbed4-2bb4-4bce-a5f0-cc22578031a2",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.960Z",
      "ceterms:ctid": "ce-d7acbed4-2bb4-4bce-a5f0-cc22578031a2",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-d7acbed4-2bb4-4bce-a5f0-cc22578031a2"
      ],
      "skos:definition": {
        "en-us": "Demonstrates competencies for the processing of new and complex professional tasks and problem settings within a scientific subject or an occupational field characterized by frequent and unpredictable changes. This requires the need to elucidate the major theories and the application of advanced specialized knowledge, research methods and approaches in various contexts."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 7"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    },
    {
      "@id": "https://credentialengineregistry.org/resources/ce-0edf49cc-8c07-42f8-b253-1ef8d39b47b4",
      "@type": "skos:Concept",
      "ceasn:dateCreated": "2020-11-17T14:34:24.964Z",
      "ceterms:ctid": "ce-0edf49cc-8c07-42f8-b253-1ef8d39b47b4",
      "https://purl.org/ctdl/terms/ctid": [
        "ce-0edf49cc-8c07-42f8-b253-1ef8d39b47b4"
      ],
      "skos:definition": {
        "en-us": "Demonstrates competencies for obtaining research findings in a scientific subject or for the development of innovative solutions and procedures in highly complex and novel problem situations within a field of occupational activity. This requires a capacity for a wide range of strategic and scientific thinking and creative action."
      },
      "skos:inScheme": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1",
      "skos:prefLabel": {
        "en-us": "Level 8"
      },
      "skos:topConceptOf": "https://credentialengineregistry.org/resources/ce-75a38ea0-319d-4748-8bb0-a6dc4c68feb1"
    }
  ]
}
